                       temp = cmax / bignum
                       x( 1, 1 ) = temp*x( 1, 1 )
                       x( 2, 1 ) = temp*x( 2, 1 )
                       xnorm = temp*xnorm
                       scale = temp*scale
                    end if
                 end if
              else
