                 ! real 1x1 system.
                 ! c = ca a - w d
                 csr = ca*a( 1, 1 ) - wr*d1
                 cnorm = abs( csr )
                 ! if | c | < smini, use c = smini
                 if( cnorm<smini ) then
                    csr = smini
                    cnorm = smini
                    info = 1
                 end if
                 ! check scaling for  x = b / c
                 bnorm = abs( b( 1, 1 ) )
                 if( cnorm<one .and. bnorm>one ) then
                    if( bnorm>bignum*cnorm )scale = one / bnorm
                 end if
                 ! compute x
                 x( 1, 1 ) = ( b( 1, 1 )*scale ) / csr
                 xnorm = abs( x( 1, 1 ) )
              else
                 ! complex 1x1 system (w is complex)
                 ! c = ca a - w d
                 csr = ca*a( 1, 1 ) - wr*d1
