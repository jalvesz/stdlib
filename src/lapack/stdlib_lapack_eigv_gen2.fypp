              work( 1 ) = lwmin
              iwork( 1 ) = liwmin
           end if
           if( info/=0 ) then
              call stdlib_xerbla( 'STRSEN', -info )
              return
           else if( lquery ) then
              return
           end if
           ! quick return if possible.
           if( m==n .or. m==0 ) then
              if( wants )s = one
