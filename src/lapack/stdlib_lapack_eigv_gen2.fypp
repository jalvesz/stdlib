                    x( 1, 1 ) = xr2
                    x( 2, 1 ) = xr1
                    x( 1, 2 ) = xi2
                    x( 2, 2 ) = xi1
                 else
                    x( 1, 1 ) = xr1
                    x( 2, 1 ) = xr2
                    x( 1, 2 ) = xi1
                    x( 2, 2 ) = xi2
                 end if
                 xnorm = max( abs( xr1 )+abs( xi1 ), abs( xr2 )+abs( xi2 ) )
                 ! further scaling if  norm(a) norm(x) > overflow
                 if( xnorm>one .and. cmax>one ) then
                    if( xnorm>bignum / cmax ) then
                       temp = cmax / bignum
                       x( 1, 1 ) = temp*x( 1, 1 )
                       x( 2, 1 ) = temp*x( 2, 1 )
                       x( 1, 2 ) = temp*x( 1, 2 )
                       x( 2, 2 ) = temp*x( 2, 2 )
                       xnorm = temp*xnorm
                       scale = temp*scale
                    end if
                 end if
              end if
           end if
           return
     end subroutine stdlib_${ri}$laln2

