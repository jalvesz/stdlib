           ! Local Arrays 
           logical(lk) :: rswap(4), zswap(4)
           integer(ilp) :: ipivot(4,4)
           real(dp) :: ci(2,2), civ(4), cr(2,2), crv(4)
           ! Intrinsic Functions 
           ! Equivalences 
           equivalence        ( ci( 1, 1 ), civ( 1 ) ),( cr( 1, 1 ), crv( 1 ) )
           ! Data Statements 
