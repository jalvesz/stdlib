                 kt = nw + 1
                 nho = ( n-nw-1 ) - kt + 1
                 kwv = nw + 2
                 nve = ( n-nw ) - kwv + 1
                 ! ==== aggressive early deflation ====
                 call stdlib_zlaqr3( wantt, wantz, n, ktop, kbot, nw, h, ldh, iloz,ihiz, z, ldz, &
                 ls, ld, w, h( kv, 1 ), ldh, nho,h( kv, kt ), ldh, nve, h( kwv, 1 ), ldh, work,&
                           lwork )
