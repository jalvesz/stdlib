                 ur11 = crv( icmax )
                 cr21 = crv( ipivot( 2, icmax ) )
                 ur12 = crv( ipivot( 3, icmax ) )
                 cr22 = crv( ipivot( 4, icmax ) )
                 ur11r = one / ur11
                 lr21 = ur11r*cr21
                 ur22 = cr22 - ur12*lr21
                 ! if smaller pivot < smini, use smini
                 if( abs( ur22 )<smini ) then
                    ur22 = smini
                    info = 1
                 end if
                 if( rswap( icmax ) ) then
                    br1 = b( 2, 1 )
                    br2 = b( 1, 1 )
                 else
                    br1 = b( 1, 1 )
                    br2 = b( 2, 1 )
                 end if
