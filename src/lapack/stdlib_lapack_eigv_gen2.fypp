                    ! .    fortunately, stdlib_dtrexc does the right thing with
                    ! .    ilst in case of a rare exchange failure. ====
                    ifst = ns
                    call stdlib_dtrexc( 'V', jw, t, ldt, v, ldv, ifst, ilst, work,info )
                    ilst = ilst + 2
                 end if
              end if
              ! ==== end deflation detection loop ====
              go to 20
           end if
