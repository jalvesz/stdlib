                    br1 = b( 1, 1 )
                    br2 = b( 2, 1 )
                    bi1 = b( 1, 2 )
                    bi2 = b( 2, 2 )
                 end if
                 br2 = br2 - lr21*br1 + li21*bi1
                 bi2 = bi2 - li21*br1 - lr21*bi1
                 bbnd = max( ( abs( br1 )+abs( bi1 ) )*( u22abs*( abs( ur11r )+abs( ui11r ) ) ),&
                           abs( br2 )+abs( bi2 ) )
