                 v( i ) = rtemp
              end do
              v( n-its+1 ) = v( n-its+1 ) - eps3*rootn
           end do
           ! failure to find eigenvector in n iterations.
           info = 1
           120 continue
           ! normalize eigenvector.
           i = stdlib_i${ci}$amax( n, v, 1 )
