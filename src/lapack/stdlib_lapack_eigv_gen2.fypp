                 xr1 = ( scale*br1 )*ur11r - xr2*( ur11r*ur12 )
                 if( zswap( icmax ) ) then
                    x( 1, 1 ) = xr2
                    x( 2, 1 ) = xr1
                 else
                    x( 1, 1 ) = xr1
                    x( 2, 1 ) = xr2
                 end if
                 xnorm = max( abs( xr1 ), abs( xr2 ) )
