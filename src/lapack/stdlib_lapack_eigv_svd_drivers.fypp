                    ! cworkspace: need   0
                    ! rworkspace: need   m [e] + bdspac
                    call stdlib_dbdsdc( 'U', 'N', m, s, rwork( ie ), dum,1,dum,1,dum, idum, rwork(&
                               nrwork ), iwork, info )
