                          call stdlib_${ci}$lacpy( 'F', m, blk, work( iu ), ldwrku,a( 1, i ), lda )
                                    
                       end do
                    else
                       ! insufficient workspace for a fast algorithm
                       itau = 1
                       iwork = itau + m
                       ! compute a=l*q
                       ! (cworkspace: need 2*m, prefer m+m*nb)
                       ! (rworkspace: 0)
                       call stdlib_${ci}$gelqf( m, n, a, lda, work( itau ),work( iwork ), lwork-iwork+&
