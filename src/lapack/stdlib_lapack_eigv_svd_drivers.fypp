                    call stdlib_${ci}$gelqf( m, n, a, lda, work( itau ), work( iwork ),lwork-iwork+1, &
                              ierr )
                    ! zero out above l
                    if (m>1) call stdlib_${ci}$laset( 'U', m-1, m-1, czero, czero, a( 1, 2 ),lda )
                    ie = 1
                    itauq = 1
