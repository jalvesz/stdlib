                       ! (cworkspace: 0)
                       ! (rworkspace: need bdspac)
                       call stdlib_${ci}$bdsqr( 'L', m, n, 0, 0, s, rwork( ie ), a, lda,cdum, 1, cdum, &
                                 1, rwork( irwork ), info )
