                       call stdlib_${ci}$lacpy( 'L', m, m, a, lda, work( ir ), ldwrkr )
                       call stdlib_${ci}$laset( 'U', m-1, m-1, czero, czero,work( ir+ldwrkr ), ldwrkr )
                                 
