                    ! (rworkspace: 0)
                    call stdlib_cungbr( 'P', n, n, n, a, lda, work( itaup ),work( iwork ), lwork-&
                              iwork+1, ierr )
                 end if
                 irwork = ie + n
                 if( wntuas .or. wntuo )nru = m
                 if( wntun )nru = 0
                 if( wntvas .or. wntvo )ncvt = n
