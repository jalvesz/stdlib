                          iwork = itau + m
                          ! compute a=l*q
                          ! (workspace: need m*m + 2*m, prefer m*m + m + m*nb)
                          call stdlib_${ri}$gelqf( m, n, a, lda, work( itau ),work( iwork ), lwork-&
