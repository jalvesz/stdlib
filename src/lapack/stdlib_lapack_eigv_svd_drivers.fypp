                       wrkbl = max( wrkbl, n+lwork_zungqr_n )
                       wrkbl = max( wrkbl, 2*n+lwork_zgebrd )
                       wrkbl = max( wrkbl, 2*n+lwork_zungbr_q )
                       maxwrk = n*n + wrkbl
                       minwrk = 2*n + m
                    else if( wntus .and. wntvo ) then
                       ! path 5 (m much larger than n, jobu='s', jobvt='o')
                       wrkbl = n + lwork_zgeqrf
                       wrkbl = max( wrkbl, n+lwork_zungqr_n )
                       wrkbl = max( wrkbl, 2*n+lwork_zgebrd )
                       wrkbl = max( wrkbl, 2*n+lwork_zungbr_q )
